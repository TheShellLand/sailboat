{
  "nics": [
    {
      "interface": "$INTERFACE",
      "nic_tag": "$NICTAG",
      "model": "virtio, e1000, rtl8139",
      "ip": "$IPADDRESS",
      "gateway": "$GATEWAY",
      "netmask": "$NETMASK",
      "primary": true
    }
  ],
  "resolvers": [
    "$DNS_PRIMARY",
    "$DNS_SECONDARY"
  ],
  "disks": [
    {
      "image_uuid": "$IMAGE_UUID",
      "image_name": "$IMAGE_NAME",
      "image_size": $IMAGE_SIZE,
      "model": "virtio | ide | scsi",
      "compression": "on,off,lzjb,gzip,gzip-N,zle",
      "boot": true
    }
  ],
  "vcpus": "number of VCPU",
  "ram": number
  in
  MB,
  "autoboot": true
  |
  false,
  "alias": "string",
  "brand": "kvm"
}